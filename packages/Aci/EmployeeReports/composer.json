{
    "name": "aci/employee-reports",
    "version": "dev-main",
    "description": "Laravel package for customizable employee PDF reports",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aci\\EmployeeReports\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aci\\EmployeeReports\\EmployeeReportsServiceProvider"
            ],
            "aliases": {
                "EmployeeReports": "Aci\\EmployeeReports\\Facades\\EmployeeReports"
            }
        }
    },
    "require": {
        "php": "^8.1|^8.2",
        "barryvdh/laravel-dompdf": "^3.1"
    }
}
